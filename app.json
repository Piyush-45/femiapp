{
  "expo": {
    "name": "femihub",
    "slug": "femihub",
    "version": "1.0.0",
    "orientation": "portrait",
    // "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      // "backgroundColor": "red"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",{"fonts":["../assets/fonts/Amiri-Bold.ttf"]}
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
